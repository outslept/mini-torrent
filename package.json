{
  "name": "mini-torrent",
  "version": "0.0.0",
  "type": "module",
  "description": "Mini Torrent inspired by craft your own X",
  "packageManager": "pnpm@10.2.1",
  "author": "outslept",
  "keywords": [
    "torrent",
    "bittorrent",
    "craftyourownx"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/outslept/mini-torrent"
  },
  "homepage": "https://github.com/outslept/mini-torrent#readme",
  "bugs": {
    "url": "https://github.com/outslept/mini-torrent/issues"
  },
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx index.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@types/bencode": "^2.0.4",
    "@types/node": "^22.13.5",
    "axios": "^1.7.9",
    "bencode": "^4.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1"
  }
}
